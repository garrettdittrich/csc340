<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorAccent"
    android:orientation="vertical"
    tools:context=".proposal">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="398dp"
            android:layout_height="600dp"
            android:layout_marginStart="6dp"
            android:layout_marginTop="66dp"
            android:layout_marginEnd="6dp"
            android:layout_marginBottom="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@color/common_google_signin_btn_text_dark_default" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="90dp"
            android:layout_height="23dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="183dp"
            android:layout_marginEnd="309dp"
            android:layout_marginBottom="525dp"
            android:text="Email"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/email"
            android:layout_width="388dp"
            android:layout_height="29dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="206dp"
            android:layout_marginEnd="11dp"
            android:layout_marginBottom="496dp"
            android:background="?attr/colorButtonNormal"
            android:ems="10"
            android:inputType="text"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="137dp"
            android:layout_height="25dp"
            android:layout_marginStart="13dp"
            android:layout_marginTop="251dp"
            android:layout_marginEnd="261dp"
            android:layout_marginBottom="455dp"
            android:text="State, and Country"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/location"
            android:layout_width="388dp"
            android:layout_height="29dp"
            android:layout_marginStart="13dp"
            android:layout_marginTop="276dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="426dp"
            android:background="?attr/colorButtonNormal"
            android:ems="10"
            android:inputType="text"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="81dp"
            android:layout_height="27dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="341dp"
            android:layout_marginEnd="318dp"
            android:layout_marginBottom="363dp"
            android:text="Username"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/time"
            android:layout_width="388dp"
            android:layout_height="29dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="368dp"
            android:layout_marginEnd="11dp"
            android:layout_marginBottom="334dp"
            android:background="?android:attr/colorButtonNormal"
            android:ems="10"
            android:inputType="text"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="60dp"
            android:layout_height="27dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="473dp"
            android:layout_marginEnd="339dp"
            android:layout_marginBottom="231dp"
            android:background="@color/common_google_signin_btn_text_dark_default"
            android:text="Birthday"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/birthday"
            android:layout_width="388dp"
            android:layout_height="29dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="500dp"
            android:layout_marginEnd="11dp"
            android:layout_marginBottom="202dp"
            android:background="?attr/colorButtonNormal"
            android:text="Choosen Birthday"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/datepick"
            android:layout_width="131dp"
            android:layout_height="60dp"
            android:layout_marginStart="140dp"
            android:layout_marginTop="413dp"
            android:layout_marginEnd="140dp"
            android:layout_marginBottom="258dp"
            android:text="Choose Birthday"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/submit"
            android:layout_width="131dp"
            android:layout_height="60dp"
            android:layout_marginStart="140dp"
            android:layout_marginTop="541dp"
            android:layout_marginEnd="140dp"
            android:layout_marginBottom="130dp"
            android:text="Submit"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="384dp"
            android:layout_height="80dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="89dp"
            android:layout_marginEnd="18dp"
            android:layout_marginBottom="562dp"
            android:fontFamily="monospace"
            android:text="Sign up"
            android:textAlignment="center"
            android:textSize="36sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </android.support.constraint.ConstraintLayout>
</LinearLayout>